<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>servlets-app</display-name>

	<!-- This is comment -->
	<servlet>
		<servlet-name>demo</servlet-name>
		<servlet-class>com.hsbc.tr.web.Servlet1</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>form</servlet-name>
		<servlet-class>com.hsbc.tr.web.FormServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>processData</servlet-name>
		<servlet-class>com.hsbc.tr.web.ProcessInputServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>demo</servlet-name>
		<url-pattern>/test</url-pattern>

	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>form</servlet-name>
		<url-pattern>/myform</url-pattern>

	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>processData</servlet-name>
		<url-pattern>/process</url-pattern>

	</servlet-mapping>

</web-app>